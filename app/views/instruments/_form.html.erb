<%= form_for @instrument do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, :required => true %>

  <%= f.label :family %>
  <%= f.text_field :family %>

  <%= f.label :country %>
  <%= f.text_field :country %>

  <%= f.label :image %>
  <%= f.url_field :image %>

  <%= f.submit %>
<% end %>
